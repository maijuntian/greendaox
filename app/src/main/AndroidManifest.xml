<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mai.greendaox">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".MApplication">
        <activity android:name=".activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.BasisActivity" />
        <activity android:name=".activity.OneToOneActivity" />
        <activity android:name=".activity.OneToManyActivity" />
        <activity android:name=".activity.ManyToManyActivity" />
        <activity android:name=".activity.CascadeActivity" />
        <activity android:name=".activity.LazyActivity" />
    </application>

</manifest>